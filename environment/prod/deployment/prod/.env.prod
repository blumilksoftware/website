MAIL_MAILER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=465
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=no-reply@blumilk.pl
MAIL_FROM_NAME=Blumilk

CONTACT_FORM_NOTIFY_EMAIL=katarzyna.nowak@blumilk.pl

SESSION_SECURE_COOKIE=true
