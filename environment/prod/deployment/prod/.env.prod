COMPOSE_PROJECT_NAME=blumilk-website-prod

BLUMILK_WEBSITE_HOST_NAME=blumilk.pl
APP_URL="https://${BLUMILK_WEBSITE_HOST_NAME}"

APP_NAME=blumilk-website-prod
APP_ENV=prod
APP_DEBUG=false

DB_CONNECTION=pgsql
DB_HOST=blumilk-website-prod-database
DB_PORT=5432

